      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{36FE53FE-F822-43F5-8232-C93E35E5CBD2}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="MERGE Fact_Clinical AS target&#xA;USING dbo.Staging_Clinical AS source&#xA;    ON target.visit_id = source.visit_id&#xA;&#xA;WHEN MATCHED AND (&#xA;    target.patient_sk &lt;&gt; source.patient_sk OR&#xA;    target.department_sk &lt;&gt; source.department_sk OR&#xA;    target.doctor_sk &lt;&gt; source.doctor_sk OR&#xA;    target.clinical_date_sk &lt;&gt; source.clinical_date_sk OR&#xA;    target.number_of_diagnoses &lt;&gt; source.number_of_diagnoses OR&#xA;    target.severity_score &lt;&gt; source.severity_score OR&#xA;    ISNULL(target.prescription_drug_id, -1) &lt;&gt; ISNULL(source.prescription_drug_id, -1) OR&#xA;    ISNULL(target.allergy_sk, -1) &lt;&gt; ISNULL(source.allergy_sk, -1) OR&#xA;    ISNULL(target.lab_test_sk, -1) &lt;&gt; ISNULL(source.lab_test_sk, -1) OR&#xA;    ISNULL(target.radiology_sk, -1) &lt;&gt; ISNULL(source.radiology_sk, -1)&#xA;) THEN&#xA;    UPDATE SET&#xA;        target.patient_sk = source.patient_sk,&#xA;        target.department_sk = source.department_sk,&#xA;        target.doctor_sk = source.doctor_sk,&#xA;        target.clinical_date_sk = source.clinical_date_sk,&#xA;        target.number_of_diagnoses = source.number_of_diagnoses,&#xA;        target.chronic_condition_flag = source.chronic_condition_flag,&#xA;        target.allergy_count = source.allergy_count,&#xA;        target.has_high_severity_allergy = source.has_high_severity_allergy,&#xA;        target.vital_blood_pressure_systolic = source.vital_blood_pressure_systolic,&#xA;        target.vital_blood_pressure_diastolic = source.vital_blood_pressure_diastolic,&#xA;        target.vital_heart_rate = source.vital_heart_rate,&#xA;        target.vital_temperature = source.vital_temperature,&#xA;        target.vital_blood_oxygen_level = source.vital_blood_oxygen_level,&#xA;        target.vital_respiratory_rate = source.vital_respiratory_rate,&#xA;        target.vital_status_flag = source.vital_status_flag,&#xA;        target.number_of_lab_tests = source.number_of_lab_tests,&#xA;        target.number_of_abnormal_results = source.number_of_abnormal_results,&#xA;        target.abnormal_result_flag = source.abnormal_result_flag,&#xA;        target.has_radiology = source.has_radiology,&#xA;        target.radiology_types_count = source.radiology_types_count,&#xA;        target.follow_up_required_flag = source.follow_up_required_flag,&#xA;        target.general_health_status = source.general_health_status,&#xA;        target.severity_score = source.severity_score,&#xA;        target.prescription_drug_id = source.prescription_drug_id,&#xA;        target.allergy_sk = source.allergy_sk,           -- ✅ جديد&#xA;        target.lab_test_sk = source.lab_test_sk,         -- ✅ جديد&#xA;        target.radiology_sk = source.radiology_sk        -- ✅ جديد&#xA;&#xA;WHEN NOT MATCHED THEN&#xA;    INSERT (&#xA;        visit_id,&#xA;        patient_sk,&#xA;        department_sk,&#xA;        doctor_sk,&#xA;        clinical_date_sk,&#xA;        number_of_diagnoses,&#xA;        chronic_condition_flag,&#xA;        allergy_count,&#xA;        has_high_severity_allergy,&#xA;        vital_blood_pressure_systolic,&#xA;        vital_blood_pressure_diastolic,&#xA;        vital_heart_rate,&#xA;        vital_temperature,&#xA;        vital_blood_oxygen_level,&#xA;        vital_respiratory_rate,&#xA;        vital_status_flag,&#xA;        number_of_lab_tests,&#xA;        number_of_abnormal_results,&#xA;        abnormal_result_flag,&#xA;        has_radiology,&#xA;        radiology_types_count,&#xA;        follow_up_required_flag,&#xA;        general_health_status,&#xA;        severity_score,&#xA;        prescription_drug_id,&#xA;        allergy_sk,           -- ✅ جديد&#xA;        lab_test_sk,          -- ✅ جديد&#xA;        radiology_sk          -- ✅ جديد&#xA;    )&#xA;    VALUES (&#xA;        source.visit_id,&#xA;        source.patient_sk,&#xA;        source.department_sk,&#xA;        source.doctor_sk,&#xA;        source.clinical_date_sk,&#xA;        source.number_of_diagnoses,&#xA;        source.chronic_condition_flag,&#xA;        source.allergy_count,&#xA;        source.has_high_severity_allergy,&#xA;        source.vital_blood_pressure_systolic,&#xA;        source.vital_blood_pressure_diastolic,&#xA;        source.vital_heart_rate,&#xA;        source.vital_temperature,&#xA;        source.vital_blood_oxygen_level,&#xA;        source.vital_respiratory_rate,&#xA;        source.vital_status_flag,&#xA;        source.number_of_lab_tests,&#xA;        source.number_of_abnormal_results,&#xA;        source.abnormal_result_flag,&#xA;        source.has_radiology,&#xA;        source.radiology_types_count,&#xA;        source.follow_up_required_flag,&#xA;        source.general_health_status,&#xA;        source.severity_score,&#xA;        source.prescription_drug_id,&#xA;        source.allergy_sk,           -- ✅ جديد&#xA;        source.lab_test_sk,          -- ✅ جديد&#xA;        source.radiology_sk          -- ✅ جديد&#xA;    );&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
