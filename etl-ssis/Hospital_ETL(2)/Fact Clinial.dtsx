              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    v.visit_id,
    v.patient_id,
    v.department_id,
    appt.doctor_id,
    CAST(v.visit_date AS DATE) AS clinical_date,

    -- 🟢 معلومات الوصفة والدواء
    p.prescription_id,
    pi.item_id AS prescription_drug_id,
    pi.medicine_name AS prescription_medicine_name,
    1 AS number_of_prescription_items,  -- ثابت لأن كل وصفة لها دواء واحد فقط

    -- 🟢 الحساسية والأمراض المزمنة
    COUNT(DISTINCT a.allergy_id) AS allergy_count,
    CASE WHEN MAX(a.severity) = 'Severe' THEN 1 ELSE 0 END AS has_high_severity_allergy,
    MAX(a.allergy_id) AS allergy_id,  -- ✅ جديد: لربط Dim_Allergy
    CASE WHEN COUNT(DISTINCT cc.condition_id) &gt; 0 THEN 1 ELSE 0 END AS chronic_condition_flag,

    -- 🟢 التشخيص
    COUNT(DISTINCT d.diagnosis_id) AS number_of_diagnoses,

    -- 🟢 القياسات الحيوية
    vv.blood_pressure_systolic,
    vv.blood_pressure_diastolic,
    vv.heart_rate,
    vv.body_temperature,
    vv.blood_oxygen_level,
    vv.respiratory_rate,
    CASE 
        WHEN vv.blood_oxygen_level &lt; 90 
          OR vv.heart_rate &gt; 120 
          OR vv.body_temperature &gt; 38 THEN 1 
        ELSE 0 
    END AS vital_status_flag,

    -- 🟢 التحاليل
    COUNT(DISTINCT tr.lab_test_id) AS number_of_lab_tests,
    SUM(CASE 
            WHEN TRY_CAST(tr.attribute_value AS FLOAT) NOT BETWEEN 70 AND 120 THEN 1 
            ELSE 0 
        END) AS number_of_abnormal_results,
    CASE 
        WHEN SUM(CASE 
                    WHEN TRY_CAST(tr.attribute_value AS FLOAT) NOT BETWEEN 70 AND 120 THEN 1 
                    ELSE 0 
                END) &gt; 0 THEN 1 
        ELSE 0 
    END AS abnormal_result_flag,
    MAX(tr.lab_test_id) AS lab_test_id,  -- ✅ جديد: لربط Dim_LabTest

    -- 🟢 الأشعة
    CASE WHEN COUNT(DISTINCT r.report_id) &gt; 0 THEN 1 ELSE 0 END AS has_radiology,
    COUNT(DISTINCT r.radiology_type_id) AS radiology_types_count,
    MAX(r.radiology_type_id) AS radiology_type_id,  -- ✅ جديد: لربط Dim_Radiology

    -- 🟢 المتابعة
    mr.follow_up_required AS follow_up_required_flag,
    mr.general_health_status,

    -- 🟢 شدة الحالة
    (
        CASE WHEN mr.general_health_status = 'Critical' THEN 3
             WHEN mr.general_health_status = 'Fair' THEN 2
             ELSE 1
        END
        + CASE WHEN vv.blood_oxygen_level &lt; 90 THEN 2 ELSE 0 END
    ) AS severity_score

FROM Visit.Visits v

-- 🟢 ربط بالموعد للحصول على الدكتور
LEFT JOIN scheduling.Appointments appt 
    ON appt.patient_id = v.patient_id 
    AND appt.appointment_time = (
        SELECT MAX(appointment_time)
        FROM scheduling.Appointments ap
        WHERE ap.patient_id = v.patient_id
    )

-- 🟢 جداول طبية مختلفة
LEFT JOIN Visit.Medical_Records mr ON mr.visit_id = v.visit_id
LEFT JOIN Visit.Visit_Vitals vv ON vv.visit_id = v.visit_id
LEFT JOIN Visit.Diagnoses d ON d.visit_id = v.visit_id
LEFT JOIN clinical.Test_Result_Details tr ON tr.visit_id = v.visit_id
LEFT JOIN clinical.Radiology_Reports r ON r.visit_id = v.visit_id
LEFT JOIN Visit.Prescriptions p ON p.visit_id = v.visit_id
LEFT JOIN Visit.PrescriptionItems pi ON pi.prescription_id = p.prescription_id

-- 🟢 المرضى
LEFT JOIN Patient.Allergies a ON a.patient_id = v.patient_id
LEFT JOIN Patient.Chronic_Conditions cc ON cc.patient_id = v.patient_id

GROUP BY
    v.visit_id,
    v.patient_id,
    v.department_id,
    appt.doctor_id,
    CAST(v.visit_date AS DATE),
    mr.follow_up_required,
    mr.general_health_status,
    vv.blood_pressure_systolic,
    vv.blood_pressure_diastolic,
    vv.heart_rate,
    vv.body_temperature,
    vv.blood_oxygen_level,
    vv.respiratory_rate,
    p.prescription_id,
    pi.item_id,
    pi.medicine_name;</property>
